<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:padding="5dp"
    tools:context=".HomeActivity">

    <View
        android:id="@+id/view"
        android:layout_width="0dp"
        android:layout_height="140dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/stroke"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </View>

    <ImageButton
        android:id="@+id/circularButton"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/solid"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.54" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="All Categories List"
        android:textColor="@color/white"
        android:textSize="23sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/circularButton"
        app:layout_constraintStart_toEndOf="@+id/circularButton"
        app:layout_constraintTop_toTopOf="@+id/circularButton"
        app:layout_constraintVertical_bias="0.542" />

    <ImageView
        android:id="@+id/homeArrow"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.561"
        app:srcCompat="@drawable/right_arrow__1_" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/dress"
        app:layout_constraintBottom_toBottomOf="@+id/circularButton"
        app:layout_constraintEnd_toEndOf="@+id/circularButton"
        app:layout_constraintStart_toStartOf="@+id/circularButton"
        app:layout_constraintTop_toTopOf="@+id/circularButton" />

    <View
        android:id="@+id/view2"
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:layout_marginTop="@dimen/_16sdp"
        android:background="@drawable/stroke"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view" />
    <ImageButton
        android:id="@+id/shareApp"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/solid"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2"
        app:layout_constraintVertical_bias="0.421" />

    <ImageButton
        android:id="@+id/share"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/share"
        app:layout_constraintBottom_toBottomOf="@+id/shareApp"
        app:layout_constraintEnd_toEndOf="@+id/shareApp"
        app:layout_constraintStart_toStartOf="@+id/shareApp"
        app:layout_constraintTop_toTopOf="@+id/shareApp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Share"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintStart_toEndOf="@+id/shareApp"
        app:layout_constraintTop_toTopOf="@+id/view2" />

    <View
        android:id="@+id/view3"
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:background="@drawable/stroke"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toStartOf="@+id/view4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2"
        app:layout_constraintVertical_bias="0.0" />
    <ImageButton
        android:id="@+id/rate"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="@dimen/_6sdp"
        android:background="@drawable/solid"
        app:layout_constraintBottom_toBottomOf="@+id/view3"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toTopOf="@+id/view3"
        app:layout_constraintVertical_bias="0.421" />

    <ImageButton
        android:id="@+id/rateimg"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/rate"
        app:layout_constraintBottom_toBottomOf="@+id/rate"
        app:layout_constraintEnd_toEndOf="@+id/rate"
        app:layout_constraintStart_toStartOf="@+id/rate"
        app:layout_constraintTop_toTopOf="@+id/rate" />


    <TextView
        android:id="@+id/textrate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_4sdp"
        android:text="Rating"
        app:layout_constraintVertical_bias="0.489"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="@+id/view3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/rate"
        app:layout_constraintTop_toTopOf="@+id/view3"
        />

    <View
        android:id="@+id/view4"
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:background="@drawable/stroke"
        app:layout_constraintBottom_toBottomOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/view3"
        app:layout_constraintTop_toTopOf="@+id/view3" />

    <ImageButton
        android:id="@+id/saved"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="@dimen/_6sdp"
        android:background="@drawable/solid"
        app:layout_constraintBottom_toBottomOf="@+id/view4"
        app:layout_constraintStart_toStartOf="@+id/view4"
        app:layout_constraintTop_toTopOf="@+id/view4"
        app:layout_constraintVertical_bias="0.421" />

    <ImageButton
        android:id="@+id/savedimg"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/bookmarks"
        app:layout_constraintBottom_toBottomOf="@+id/saved"
        app:layout_constraintEnd_toEndOf="@+id/saved"
        app:layout_constraintStart_toStartOf="@+id/saved"
        app:layout_constraintTop_toTopOf="@+id/saved" />

    <TextView
        android:id="@+id/textsave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_8sdp"
        android:text="Saved"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/view4"
        app:layout_constraintEnd_toEndOf="@+id/view4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/saved"
        app:layout_constraintTop_toTopOf="@+id/view4"
        app:layout_constraintVertical_bias="0.489" />

</androidx.constraintlayout.widget.ConstraintLayout>